{
    "name": "YoutubeHelper",
    "version": "2.0",
    "manifest_version": 3,
    "description": "Remove advertisements injected in the youtube videos and webpages",
    "background": {
      "service_worker": "main.js"
    },
    "permissions": [
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "*://*.youtube.com/watch?v=*"
    ]
  }
  