{
    "name": "YoutubeHelper",
    "version": "1.1",
    "manifest_version": 2,
    "description": "Skip out advertisements injected in the youtube videos",
    "background": {
      "service_worker": "main.js"
    },
    "permissions": [
      "tabs",
      "*://*.youtube.com/watch?v=*"
    ]
  }
  